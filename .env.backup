# Flash API Configuration
REACT_APP_FLASH_API_BASE_URL=https://api.flashapp.me/graphql
REACT_APP_FLASH_API_KEY=your_api_key_here
REACT_APP_FYGARO_BASE_URL=https://fygaro.com

NODE_ENV=development

# Feature Flags - Enable Flash API features
REACT_APP_ENABLE_FLASH_SEND=true
REACT_APP_ENABLE_BANK_SETTLE=true
REACT_APP_ENABLE_BANK_TOPUP=true
REACT_APP_ENABLE_FYGARO_TOPUP=true

# Flash API Authentication
# The API uses Bearer token authentication in format: "Authorization: Bearer ory..."
# You'll need to provide your actual Bearer token here
REACT_APP_FLASH_AUTH_TOKEN=ory_your_actual_token_here